{
  "name": "speck-dynamodb-repository",
  "version": "0.1.3",
  "description": "Repositories using Speck",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard -v \"src/**/*.js\"",
    "lint:fix": "standard --fix \"src/**/*.js\"",
    "lint:watch": "watch -p \"+(src)/**/*.js\" -c \"yarn lint\"",
    "test": "npm run lint && NODE_ENV=test NODE_PATH=./ mocha \"src/**/*.test.js\" --colors --reporter spec",
    "test:watch": "npm test -- --watch",
    "coverage": "npm run lint && nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielbdias/speck-dynamodb-repository"
  },
  "keywords": [
    "Javascript",
    "Entities",
    "Repositories",
    "DynamoDB"
  ],
  "author": "Daniel Baptista",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/danielbdias/speck-dynamodb-repository/issues"
  },
  "engines": {
    "node": "6.10.2",
    "npm": "3.10.10"
  },
  "dependencies": {
    "aws-sdk": "^2.199.0",
    "speck-entity": "^0.1.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "joi": "^9.1.1",
    "chai": "^3.5.0",
    "mocha": "^3.1.1",
    "nyc": "^10.2.0",
    "rosie": "^1.6.0",
    "sinon": "^2.1.0",
    "standard": "^8.3.0",
    "watch-cli": "^0.2.1"
  },
  "nyc": {
    "statements": 95,
    "branches": 95,
    "functions": 95,
    "lines": 95,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*",
      "src/**/*.test.js",
      "tests/**/*"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  }
}
